{
    "name": "heimseiten/contao-article-list-with-css-classes-and-ids-bundle",
    "description": "Fügt die vergebenen CSS Klassen und IDs hübsch in die Artikelliste ein.",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "heimseiten.de - Webdesign aus Köln",
            "email": "info@heimseiten.de",
            "homepage": "https://www.heimseiten.de/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimseiten/contao-article-list-with-css-classes-and-ids-bundle/issues",
        "source": "https://github.com/heimseiten/contao-article-list-with-css-classes-and-ids-bundle"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "contao/test-case": "^4.5",
        "phpunit/phpunit": "^9.3",
        "symfony/http-client": "5.4.*",
        "symfony/phpunit-bridge": "^5.4",
        "contao/easy-coding-standard": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Heimseiten\\ContaoArticleListWithCssClassesAndIdsBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Heimseiten\\ContaoArticleListWithCssClassesAndIdsBundle\\ContaoManager\\Plugin"
    }
}
